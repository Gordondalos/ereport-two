<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <title>{% block title %}E-report{% endblock %}</title>

        {% block stylesheets %}
            {% stylesheets
            'css/font-awesome.css'
            'css/main.css'
            filter='cssrewrite' output='css/compiled/app.css' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}

    </head>
    <body>
        {% block body %}
            <div class="container">
                <div class="row">

                </div>
            </div>

            <div class="container-fluid p-x-0 p-t-2">
                <div class="row">
                    <div class="col-md-1">
                        {% block nav %}
                            {%  include ':default:navigation.html.twig' %}
                        {% endblock %}
                    </div>
                    <div class="col-md-11 p-r-3">
                        {% block content %}

                        {% endblock %}
                    </div>
                </div>
            </div>



        {% endblock %}

        {% block javascripts %}
            {% javascripts
            'js/jquery.min.js'
            'js/tether.min.js'
            'js/bootstrap.min.js'
            'js/main.js'
            output='js/compiled/app.js' %}
            <script src="{{ asset_url }}"></script>

            {% endjavascripts %}
        {% endblock %}

    </body>
</html>
