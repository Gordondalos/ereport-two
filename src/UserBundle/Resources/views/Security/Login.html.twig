                                <form class="p-y-3 p-r-2 pos-relative" action="{{ path("fos_user_security_check") }}" method="post">
                                    <input type="hidden" name="_csrf_token" value="{{ csrf_token }}" />
                                    <div class="row">
                                        <div class="col-xs-4 p-r-0 text-xs-right"><label for="mail">Логин:</label></div>
                                        <div class="col-xs-8">
                                            <input class="form-control" type="text" id="username" name="_username" value="{{ last_username }}" required="required" placeholder="name@example.com" />
                                            {#<input class="form-control" id="mail" type="email" placeholder="name@example.com">#}
                                        </div>
                                    </div>
                                    <div class="row p-t-1">
                                        <div class="col-xs-4 p-r-0 text-xs-right"><label for="password">Пароль:</label></div>
                                        <div class="col-xs-8">
                                            <input class="form-control" type="password" id="password" name="_password" required="required" placeholder="******"/>
                                            {#<input class="form-control" id="password" type="password" placeholder="******">#}
                                        </div>

                                    </div>

                                    <div class="row p-t-1">
                                        <div class="col-xs-4 p-r-0 text-xs-right">
                                            <input type="checkbox" id="remember_me" name="_remember_me" value="on" />
                                        </div>
                                        <div class="col-xs-8">
                                            <label for="remember_me">{{ 'security.login.remember_me'|trans }}</label>
                                        </div>
                                    </div>

                                    <button type="submit" id="_submit" name="_submit" class="specbutton bg-success btn"><i class="fa fa-arrow-right white"></i></button>
                                </form>

